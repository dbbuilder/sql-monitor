          "rawQuery": true,
          "rawSql": "-- Object Information Section\nSELECT\n    'Object Information' AS Section,\n    s.ServerName,\n    com.DatabaseName,\n    com.SchemaName,\n    com.ObjectName,\n    com.ObjectType,\n    com.CreatedDate,\n    com.ModifiedDate,\n    com.LineCount,\n    com.CharacterCount,\n    com.DependencyCount,\n    com.LastRefreshTime\nFROM dbo.CodeObjectMetadata com\nINNER JOIN dbo.Servers s ON com.ServerID = s.ServerID\nWHERE s.ServerName = '${ServerName}'\n  AND com.DatabaseName = '${DatabaseName}'\n  AND com.SchemaName = '${SchemaName}'\n  AND com.ObjectName = '${ObjectName}';",
          "refId": "A"
        }
      ],
      "title": "Object Information",
      "type": "table"
