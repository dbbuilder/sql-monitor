        {
          "datasource": "MonitoringDB",
          "format": "time_series",
          "rawSql": "SELECT\n  mt.CalculationDate AS time,\n  mt.GrowthPercentPerDay / 100.0 AS value,\n  s.ServerName + ' - ' + mt.MetricCategory + '.' + mt.MetricName AS metric\nFROM dbo.MetricTrends mt\nINNER JOIN dbo.Servers s ON mt.ServerID = s.ServerID\nWHERE mt.TrendPeriod = '30day'\n  AND mt.MetricCategory IN ('Memory', 'Disk', 'Connections')\n  AND mt.CalculationDate >= DATEADD(DAY, -30, GETUTCDATE())\n  AND ABS(mt.GrowthPercentPerDay) > 0.1\nORDER BY time;",
          "refId": "A"
        }
      ],
      "title": "Growth Rate Trends (% per day)",
      "type": "timeseries"
